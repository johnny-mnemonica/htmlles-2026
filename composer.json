{
    "name": "johnny-mnemonica/htmlles-2024",
    "description": "HTMLLES Festival 2024",
    "type": "project",
    "authors": [
    {
        "name": "Monica Rekas",
        "email": "hello@monicarekas.com",
        "homepage": "https://monicarekas.com"
    }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        },
        {
            "type":"composer",
            "url":"https://connect.advancedcustomfields.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "installer-paths": {
            "../plugins/{$name}": ["type:wordpress-plugin"]
        }
    },
    "require": {
        "timber/timber": "^2.0",
        "wpackagist-plugin/classic-editor": "^1.6",
        "wpackagist-plugin/duplicator": "^1.5",
        "wpackagist-plugin/advanced-custom-fields": "^6.2",
        "wpackagist-plugin/siteorigin-panels": "^2.29",
        "wpengine/advanced-custom-fields-pro": "^6.2",
        "wpackagist-plugin/acf-extended": "^0.8.9"
    }
}
